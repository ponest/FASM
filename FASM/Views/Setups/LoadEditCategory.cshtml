@model FASM_EN.Setups.Categories

<div id="Editstatus-area"></div>
<form class="form" role="form">
    <div class="form-group">
        @Html.LabelFor(m => m.CategoryName, new { @class = "col-md-6 control-label" })
        @Html.TextBoxFor(m => m.CategoryName, new { @class = "form-control", id = "EditCategoryName" })
        @Html.HiddenFor(m => m.CategoryId, new { id = "EditCategoryId" })
    </div>
</form>
